package com.executors;
import java.util.concurrent.*;

public class MyApp implements Runnable {

	@Override
	public void run() {
		// TODO Auto-generated method stub
		perform();
	}
	void perform() {
		for(int i = 0; i < 5; i++) {
			System.out.println("Hello\t" + i +"\t" + Thread.currentThread());
		}
	}

}
