
public class Person {
	private int age;
	private String name;
	
	public void display() {
		System.out.println("name:-"+name+"\t" + "age:-"+age);
	}
	
	public Person() {
		age=21;
		name="Sastha";
	}
	
	public Person(int age, String name) {
		age=age;
		name=name;
	}
	//public void Person(String name, int age)
	
	public void setAge(int a) {
		age = a;
	}
	
	
}
